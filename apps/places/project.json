{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/places/src",
  "projectType": "application",
  "targets": {
    "build": {
      "outputs": ["apps/places/public", "apps/places/.netlify"],
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "remix build"
        ],
        "cwd": "apps/places"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "cross-env NODE_ENV=development netlify dev"
        ],
        "cwd": "apps/places"
      },
      "defaultConfiguration": "development",
      "configurations": {
        "production": {
          "commands": [
            "cross-env NODE_ENV=production netlify dev"
          ]
        },
        "development": {
          "commands": [
            "cross-env NODE_ENV=development netlify dev"

          ]
        }
      }
    },
    "deploy": {
      "dependsOn": [
        {
        "target": "build", "projects": "self"
        },
        {
          "target": "build", "projects": "dependencies"
        }
      ],
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["netlify deploy"],
        "cwd": "apps/places"
      }
    }
  },
  "tags": []
}
